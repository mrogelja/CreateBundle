    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="cmf_create.propel_mapper.class">Midgard\CreatePHP\Mapper\PropelMapper</parameter>
    </parameters>

    <services>

        <service id="cmf_create.propel_mapper" class="%cmf_create.propel_mapper.class%" public="false">
